{
  "name": "@mseeley/jest-cases",
  "description": "Jest utility for varying input provided to a test function",
  "repository": "https://github.com/mseeley/jest-cases.git",
  "license": "MIT",
  "main": "lib/index.js",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.15.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@mseeley/jsdoc-to-markdown": "0.1.1",
    "commitlint": "7.3.2",
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.0",
    "prettier": "1.16.4",
    "sinon": "7.2.3",
    "standard-version": "4.4.0"
  },
  "scripts": {
    "doc": "ms-jsdoc2md",
    "test": "jest",
    "test:debug": "jest --clearCache && npx -n '--inspect-brk' jest --runInBand --no-cache",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.js\"",
    "release": "standard-version"
  }
}
